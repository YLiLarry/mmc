        "${workspaceFolder}/build/linbox/include",
        "${workspaceFolder}/build/givaro/include",
        "${workspaceFolder}/build/fflas-ffpack/include",
        "${workspaceFolder}/cnma",
    ],
    "c-cpp-flylint.run": "onType",
    "c-cpp-flylint.standard": [
        "c++14"
    ],
    "c-cpp-flylint.clang.warnings": [],
    "editor.formatOnType": false,
    "editor.tabSize": 4,
    "editor.formatOnSave": true,
    "C_Cpp.clang_format_style": "{BasedOnStyle: WebKit, IndentWidth: 4}",
    "editor.wordWrap": "off",
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "build": true
    },
    "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true
    },
    "workbench.editor.enablePreview": false,
    "cSpell.enabledLanguageIds": [
